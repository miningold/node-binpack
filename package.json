{
  "name": "binpack",
  "version": "0.0.1",
  "description": "Takes an directory of images and packs them into a single bin image and returns a mapping of file names to positions and dimensions",
  "main": "index.js",
  "bin": {
    "binpack": "bin/binpack"
  },
  "directories": {
    "bin": "bin",
    "lib": "lib",
    "src": "src",
    "test": "test"
  },
  "dependencies": {
    "canvas": "0.13.x",
    "optimist": "0.3.x"
  },
  "devDependencies": {
    "coffee-script": "1.3.x",
    "mocha": "1.5.x",
    "should": "1.2.x"
  },
  "scripts": {
    "test": "node_modules\\.bin\\mocha --reporter spec --compilers coffee:coffee-script test\\"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/miningold/node-binpack.git"
  },
  "keywords": [
    "bin",
    "pack",
    "canvas",
    "images"
  ],
  "author": "Tylor Reynolds",
  "license": "BSD"
}
